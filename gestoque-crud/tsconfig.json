{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "types": ["node"],
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "tests"]
  }
  
      